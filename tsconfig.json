{
  "extends": "../../common/tsconfig.json",
  "compilerOptions": {
	"sourceMap": true,
	"outDir": "./build/",
	"target": "es5",
	"jsx": "preserve",
	"module": "commonjs",
	"noImplicitAny": true,
	"allowJs": true,
	"allowSyntheticDefaultImports": true,
	"resolveJsonModule": true
  },
  "exclude": [
	"node_modules"
  ],
  "include": [
	"./src/**/*"
  ]
}
